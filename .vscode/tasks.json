{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/SAE.CommonComponent.sln"
            ]
        },
        {
            "label": "masterBuild",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/src/SAE.CommonComponent.Master"
            ],
            "dependsOn": [
                "publishApplication",
                "publishAuthorize",
                "publishConfigServer",
                "publishIdentity",
                "publishRouting",
                "publishUser"
            ],
            "dependsOrder": "sequence"
        },
        {
            "label": "publishApplication",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "publish",
                "${workspaceFolder}/src/SAE.CommonComponent.Application",
                "-o",
                "${workspaceFolder}/plugins/Application"
            ]
        },
        {
            "label": "publishAuthorize",
            "command": "dotnet",
            "type": "shell",
            "args": [
                "publish",
                "${workspaceFolder}/src/SAE.CommonComponent.Authorize",
                "-o",
                "${workspaceFolder}/plugins/Authorize"
            ]
        },
        {
