{
  "siteConfig": {
    "oauth": {
      "appId": "localhost.dev",
      "scope": "api",
      "authority": "http://localhost:8080",
      "redirectUris": [
        "http://localhost:8000/oauth/signin-oidc"
      ],
      "postLogoutRedirectUris": [
        "http://localhost:8000/oauth/signout-callback-oidc"
      ]
    },
    "basicInfo": {
      "name": "master.dev"
    },
    "url": {
      "host": "localhost:8000",
      "signIn": "//localhost:8000/identity/login",
      "oauth": "//localhost:8000/oauth",
      "signOut": "//localhost:8000/oauth/logout"
    },
    "api": {
      "host": "http://localhost:8080",
      "menu": "http://localhost:8080/menu/tree",
      "app": "http://localhost:8080/plugin/list",
      "config": "http://localhost:8080/app/config?appid=localhost.dev&env=Development",
      "login": "http://localhost:8080/account/login",
      "bitmaps": "http://localhost:8080/.bitmaps"
    }
  },
  "nlog": {
    "autoReload": "true",
    "internalLogLevel": "Warn",
    "internalLogFile": "logs/internal-nlog.log",
    "targets": {
      "fileTarget": {
        "type": "File",
        "fileName": "logs/${shortdate}.log",
        "layout": "${level}: ${logger}[${threadid}] ${longdate}${newline}        ${message} ${exception:message,stacktrace}",
        "maxArchiveFiles": "10"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minlevel": "Trace",
        "writeTo": "FileTarget"
      }
    ]
  },
  "database": [
    {
      "name": "default",
      "provider": "mysql",
      "connectionString": "Data Source=mysql.db.lass.net;Database=sae_common_dev;User ID=root;Password=Aa123456;pooling=true;port=3306;sslmode=none;CharSet=utf8;allowPublicKeyRetrieval=true"
    }
  ],
  "mongodb": {
    "connection": "mongodb://root:Aa123456@mongo.db.lass.net:27017/",
    "db": "sae_common_dev"
  },
  "redis": {
    "connection": "redis.cache.lass.net:6379,allowadmin=true,syncTimeout=5000,password=Aa123456",
    "db": 2
  },
  "sae": {
    "config": {
      "include": [
        {
          "name": "bitmaps",
          "nodeName": "scope",
          "url": "http://localhost:8080/.bitmaps?clusterId=cw-f25ryN0Kx1u-TBklCeQ"
        }
      ]
    }
  },
  "multiTenant": {
    "claimName": "siteid",
    "headerName": "tenant",
    "strategy": "domain",
    "host": "localhost",
    "useDefaultRule": true
  },
  "plugin": {
    "path": ""
  }
}