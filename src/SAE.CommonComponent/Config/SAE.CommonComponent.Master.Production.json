  "database": [
    {
      "Name": "default",
      "Provider": "mysql",
      "ConnectionString": "Data Source=mysql.db.lass.net;Database=sae_common;User ID=root;Password=Aa123456;pooling=true;port=3306;sslmode=none;CharSet=utf8;allowPublicKeyRetrieval=true"
    }
  ],
  "nlog": {
    "autoReload": "true",
    "internalLogLevel": "Warn",
    "internalLogFile": "logs/internal-nlog.log",
    "targets": {
      "fileTarget": {
        "type": "File",
        "fileName": "logs/${shortdate}.log",
        "layout": "${level}: ${logger}[${threadid}] ${longdate}${newline}        ${message} ${exception:message,stacktrace}",
        "maxArchiveFiles": "10"
      }
    },
    "rules": [
      {
        "logger": "*",
        "minlevel": "Trace",
        "writeTo": "FileTarget"
      }
    ]
  },
  "redis": {
    "connection": "redis.cache.lass.net:6379,allowadmin=true,syncTimeout=5000,password=Aa123456",
    "db": 1
  },
  "mongodb": {
    "connection": "mongodb://root:Aa123456@mongo.db.lass.net:27017/",
    "db": "sae_common"
  },
  "sae": {
    "config": {
      "include": [
        {
          "name": "bitmaps",
          "nodeName": "scope",
          "url": "http://api.sae.com/app/resource/authorize/list?clusterId=cw-f25ryN0Kx1u-TBklCeQ"
        }
      ]
    },
    "url": "http://api.sae.com/app/config?env=Production&clusterId=cw-f25ryN0Kx1u-TBklCeQ"
  },
  "multiTenant": {
    "claimName": "siteid",
    "headerName": "tenant",
